<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/style.css" />

    <title>Document</title>
  </head>
  <body>
    <h1>Upload Your File To Google Cloud</h1>
    <form
      action="/google/googleCloud/uploadToBucket"
      method="post"
      enctype="multipart/form-data"
    >
      <div>
        <input type="file" name="file" required />
        <div><button>upload file</button></div>
      </div>
    </form>
    <a href="/google/googleCloud/logout">logOut</a>
    <h2>Uploaded Image List</h2>
    <% if(imageList == ''){ %>
    <h1>No images Uploaded</h1>
      <% } else{ %></ul> <% imageList.forEach(image => { %>
      <li style="display: inline-block">
        <a  target="_blank" href="<%=image.public_url%>"><img height="300px" width="400px" src="<%=image.public_url%>" /></a>
         <form
      action="/google/googleCloud/download/<%=image._id%>"
      method="post"
      enctype="multipart/form-data"
    >
      <div>
        <div><button>Download</button></div>
      </div>
    </form>
      </li>
     
    
    <% });%> <% } %>

   
  </body>
</html>
